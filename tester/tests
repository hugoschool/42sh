TESTS=
echo 'source /tmp/test_script.sh'
[910-END]


# Jobs more complete
[1001]
NAME="Jobs - fg bring a job in front plan"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
echo 'sleep 10 &'
echo 'jobs'
echo 'fg %1'
[1001-END]

[1002]
NAME="Jobs - fg without arg (last job)"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
echo 'sleep 5 &'
echo 'sleep 10 &'
echo 'jobs'
echo 'fg'
[1002-END]

[1003]
NAME="Jobs - fg without any job in back plan"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
echo 'fg'
[1003-END]

[1004]
NAME="Jobs - bg put a suspend job in back plan"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
echo 'sleep 10'
echo '^Z'
echo 'bg'
echo 'jobs'
[1004-END]

[1005]
NAME="Jobs - bg with specific job ID"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
echo 'sleep 5 &'
echo 'sleep 10'
echo '^Z'
echo 'jobs'
echo 'bg %2'
echo 'jobs'
[1005-END]

[1006]
NAME="Jobs - bg without arg"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
echo 'sleep 10'
echo '^Z'
echo 'bg'
echo 'jobs'
[1006-END]

[1007]
NAME="Jobs - bg with any job is suspend"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
echo 'sleep 5 &'
echo 'jobs'
echo 'bg'
[1007-END]

[1008]
NAME="Jobs - simultaneously multiple jobs gestion"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
echo 'sleep 10 &'
echo 'sleep 15 &'
echo 'sleep 20 &'
echo 'jobs'
echo 'kill %2'
echo 'jobs'
[1008-END]

[1009]
NAME="Jobs - interactive with signals"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
echo 'sleep 10'
echo '^C'
echo 'echo "After interruption"'
[1009-END]

[1010]
NAME="Jobs - jobs with pipes and redirection"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN="rm -f /tmp/jobs_out"
TESTS=
echo 'sleep 5 | sleep 10 > /tmp/jobs_out &'
echo 'jobs'
[1010-END]


