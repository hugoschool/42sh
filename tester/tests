[403-END]

[404]
NAME="unsetenv many variables"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
echo 'setenv VAR1 value1'
echo 'setenv VAR2 value2'
echo 'setenv VAR3 value3'
echo 'env | grep VAR'
echo 'unsetenv VAR1 VAR2 VAR3'
echo 'env | grep VAR'
[404-END]

[405]
